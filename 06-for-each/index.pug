doctype html
html
  head
  body
    h1 For/Each Loop
    each n,i in [1,2,3,4,5]
      p= n + ' - ' + i
    -const names = ['Dom', 'Sophie', 'Tom']
    each name in names
      p= name
    - const grades = {'Web Dev': 85, 'Software Design': 76}
    each grade, subject in grades
      p= subject + ' - ' + grade
    else 
      strong No values to loop over!
